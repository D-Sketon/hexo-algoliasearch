{
  "name": "@reimujs/hexo-algoliasearch",
  "version": "0.1.0",
  "description": "A plugin to index posts of your Hexo blog on Algolia",
  "main": "index.js",
  "scripts": {
    "install-test-blog": "cd test_blog && npm install",
    "build": "babel src --out-dir lib --ignore **/*.spec.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm test --  --coverage",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D-Sketon/hexo-algolia.git"
  },
  "keywords": [
    "hexo",
    "algolia",
    "algoliasearch",
    "search",
    "plugin"
  ],
  "author": "Louis Barranqueiro",
  "maintainers": [
    "D-Sketon"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/D-Sketon/hexo-algoliasearch/issues"
  },
  "homepage": "https://github.com/D-Sketon/hexo-algoliasearch#readme",
  "dependencies": {
    "algoliasearch": "4.20.0",
    "striptags": "3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.9",
    "@babel/eslint-parser": "7.26.8",
    "@babel/node": "7.26.0",
    "@babel/preset-env": "7.26.9",
    "babel-cli": "6.26.0",
    "babel-jest": "29.7.0",
    "eslint": "8.53.0",
    "hexo": "6.3.0",
    "jest": "29.7.0"
  },
  "peerDependencies": {
    "hexo-util": ">=2.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
